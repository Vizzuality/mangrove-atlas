{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "Transifex": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "import/no-extraneous-dependencies": [
        "error",
        {
         " devDependencies": [
            ".storybook/**",
            "stories/**"
          ]
        }
      ],
    "import/no-unresolved": "off"
  },
  "extends": "vizzuality"
}
